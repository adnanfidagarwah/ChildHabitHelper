        <include layout="@layout/shimmer_item" />

        <include layout="@layout/shimmer_item" />

        <include layout="@layout/shimmer_item" />

        <include layout="@layout/shimmer_item" />

        <include layout="@layout/shimmer_item" />

        <include layout="@layout/shimmer_item" />

        <include layout="@layout/shimmer_item" />

        <include layout="@layout/shimmer_item" />

        <include layout="@layout/shimmer_item" />
    </LinearLayout>
</com.facebook.shimmer.ShimmerFrameLayout>
